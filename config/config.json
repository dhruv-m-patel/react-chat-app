{
  "title": "React Chat App",
  "trustProxy": true,
  "logging": {
    "format": "combined",
    "options": {}
  },
  "services": {
    "pusher": {
      "chatKit": {
        "instanceLocator": "env:PUSHER_CHATKIT_INSTANCE_LOCATOR",
        "secretKey": "env:PUSHER_CHATKIT_SECRET_KEY",
        "chatroomId": "env:PUSHER_CHATKIT_CHATROOM_ID",
        "authToken": "env:PUSHER_CHATKIT_AUTH_TOKEN",
        "receiverUser": "env:PUSHER_CHATKIT_RECEIVER_USER",
      }
    }
  },
  "meddleware": {
    "logger": {
      "priority": 20,
      "module": {
        "name": "morgan",
        "arguments": [
          "config:logging.format",
          "config:logging.options"
        ]
      }
    },
    "static": {
      "priority": 21,
      "module": {
        "name": "express",
        "method": "static",
        "arguments": [
          "path:./static"
        ]
      }
    },
    "assets": {
      "priority": 22,
      "module": {
        "name": "express",
        "method": "static",
        "arguments": [
          "path:./build-static"
        ]
      }
    },
    "cookie": {
      "priority": 23,
      "module": {
        "name": "cookie-parser"
      }
    },
    "jsonBody": {
      "priority": 24,
      "module": {
        "name": "body-parser",
        "method": "json"
      }
    },
    "formBody": {
      "priority": 25,
      "module": {
        "name": "body-parser",
        "method": "urlencoded",
        "arguments": [
          { "extended": true }
        ]
      }
    },
    "routes": {
      "priority": 26,
      "module": {
        "name": "express-enrouten",
        "arguments": [
          {
            "directory": "buildpath:./server/routes"
          }
        ]
      }
    },
    "renderPage": {
      "priority": 100,
      "module": {
        "name": "buildpath:./server/middleware/renderPage",
        "method": "default"
      }
    }
  }
}
