{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "extends":  [
    "standard",
    "eslint:recommended",
    "plugin:react/recommended",
  ],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "env": {
    "jest": true
  },
  "rules": {
    "semi": [0],
    "space-before-function-paren": [0],
    "comma-dangle": [0]
  }
}
